generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
}

model UserProfile {
  id        Int      @id @default(autoincrement())
  authId    Int      @unique
  createdAt DateTime @default(now())
  email     String?  @unique
  username  String   @unique
  avatarUrl String?
}